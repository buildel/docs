{
  "openapi": "3.0.3",
  "info": {
    "title": "Buildel API",
    "version": "1.0.0"
  },
  "servers": [
    {
      "url": "https://api.buildel.ai"
    }
  ],
  "paths": {
    "/workflows": {
      "post": {
        "tags": ["Workflows"],
        "description": "Allow to create new workflow",
        "summary": "Create workflow",
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "name": {
                    "type": "string"
                  }
                }
              }
            }
          }
        },
        "responses": {
          "201": {
            "description": "Workflow created"
          },
          "400": {
            "description": "Bad request"
          }
        }
      }
    },
    "/workflows/{workflowId}": {
      "patch": {
        "tags": ["Workflows"],
        "summary": "Update workflow",
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    }
  }
}
